        "/UsdCat": {
            "start_point": true,
            "code": [
                "import os",
                "from sys import platform",
                "import subprocess",
                "# PLEASE INSURE YOUR USD ENV IS SETUP: http://graphics.pixar.com/usd/docs/USD-Tutorials.html #",
                "# USDCAT Docs: http://graphics.pixar.com/usd/docs/USD-Toolset.html#USDToolset-usdcat #",
                "",
                "# usage: usdcat [-h] [-o file] [--usdFormat usda|usdb|usdc] [-f]",
                "#               [--mask PRIMPATH[,PRIMPATH...]]",
                "#               inputFiles [inputFiles ...]",
                "",
                "if platform == \"linux\" or platform == \"linux2\":",
                "    # linux",
                "    os.system(\"msg='\\\"Press any key to continue...\\\"';gnome-terminal -e \\\"bash -c 'usdcat  ${USD_file};read -p $msg'\\\"\")",
                "elif platform == \"darwin\":",
                "    # OS X",
                "    pass",
                "elif platform == \"win32\":",
                "    # Windows...",
                "    cmd = 'start powershell -NoExit -Command \"usdcat  ${USD_file}\";Write-Host -NoNewLine \\'Press any key to continue...\\';$null = $Host.UI.RawUI.ReadKey(\\'NoEcho,IncludeKeyDown\\');exit'",
                "    subprocess.Popen(cmd, shell=True)"
            ]
        },
        "/UsdConvert": {
            "start_point": false,
            "attrs": {
                "converted_file": {
                    "type": "raw",
                    "value": "${path::}/examples/tests//HelloWorld.usdc"
                }
            },
            "code": [
                "import os",
                "from sys import platform",
                "# PLEASE INSURE YOUR USD ENV IS SETUP: http://graphics.pixar.com/usd/docs/USD-Tutorials.html #",
                "# USDCAT Docs: http://graphics.pixar.com/usd/docs/USD-Toolset.html#USDToolset-usdcat #",
                "# usage: usdcat [-h] [-o file] [--usdFormat usda|usdb|usdc] [-f]",
                "#               [--mask PRIMPATH[,PRIMPATH...]]",
                "#               inputFiles [inputFiles ...]",
                "# *** To convert your abc to usd and vice versa you need to complie your usd with the alembic flag turned on.***",
                "# https://graphics.pixar.com/usd/docs/Alembic-USD-Plugin.html",
                "",
                "if platform == \"linux\" or platform == \"linux2\":",
                "    # linux",
                "    os.system(\"msg='\\\"Press any key to continue...\\\"';gnome-terminal -e \\\"bash -c 'usdcat ${USD_file} --out ${converted_file};read -p $msg'\\\"\")",
                "elif platform == \"darwin\":",
                "    # OS X",
                "    pass",
                "elif platform == \"win32\":",
                "    # Windows..",
                "    cmd = 'start powershell -NoExit -Command \"usdcat ${USD_file} --out ${converted_file}\";Write-Host -NoNewLine \\'Press any key to continue...\\';$null = $Host.UI.RawUI.ReadKey(\\'NoEcho,IncludeKeyDown\\');exit'",
                "    subprocess.Popen(cmd, shell=True)    "
            ]
        },
